<VirtualHost *:443>
    ServerName biomerieux
    ServerAlias biomerieux.localhost
    DocumentRoot "/Users/Benoit/Sites/biomerieux-industry/web"
    ErrorLog "/usr/local/var/log/httpd/biomerieux-error_log"
    CustomLog "/usr/local/var/log/httpd/biomerieux-access_log" common
    SetEnv APPLICATION_ENV development

    SSLEngine on
    SSLCertificateFile "/etc/apache2/ssl/biomerieux.crt"
    SSLCertificateKeyFile "/etc/apache2/ssl/biomerieux.key"

    <Directory "/Users/Benoit/Sites/biomerieux-industry">
      Options Indexes FollowSymLinks
      AllowOverride All
      Order allow,deny
      Allow from all
      Require all granted
    </Directory>
</VirtualHost>

